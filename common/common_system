#!/bin/bash

[ -f ~/.common_exports ] && . ~/.common_exports
[ -f ~/.exports ] && . ~/.exports
[ -f ~/.common_aliases ] && . ~/.common_aliases
[ -f ~/.aliases ] && . ~/.aliases
echo $SHELL | grep bash > /dev/null
if [ $? -eq 0 ]; then
  echo "from bash - invoke /etc/bashrc"
  [ -f /etc/bashrc ] && . /etc/bashrc
fi

