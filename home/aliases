#!/bin/bash

#####-------------ALIASES------------#####
alias ee="export SWT_GTK3=0; ($ECLIPSE_HOME/eclipse -data /home/$USER/data/workspace/eclipse_workspace &); exit"
alias l='ls -lh --color=auto'
alias ll='ls -ahlF --color=auto'
alias ..="cd .."
alias mvnci="mvn clean install"
alias vi=vim
alias python='/usr/bin/python3'
alias go_remote="(rdesktop work_pc_ip -u work_email -0 -D -g 1366x720 &); exit"
alias go_on_main="sudo wakeonlan -i host_dynamic_dns -p 9 FC:AA:14:26:6A:20 && ssh -o ConnectTimeout=40 host_dynamic_dns"
